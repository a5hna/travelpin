<div class="board-footer d-flex justify-content-center">
  <div class="day-schedule-footer-links">
    <div class="notification d-flex justify-content-between align-items-center">
      <%= link_to '#', id: 'new-sched', data: { toggle:'modal', target:'#new-schedule'} do %>
        <i class="far fa-sticky-note"></i>
      <% end %>
      <%= link_to board_path(params[:board_id]) do %>
        <i><%= image_tag "_triangle-marker.svg", alt: "alttext" %></i>
      <% end %>
    </div>
  </div>
</div>
